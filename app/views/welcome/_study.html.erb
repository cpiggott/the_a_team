<h3>Study Groups</h3>
<div class="row">
  <div class="col-md-12">
    <% for s in Study.all %>
      <div class="col-md-4" style="padding-bottom:5px">
        <div class="thumbnail">
          <!--<img data-src="holder.js/300x200" alt="...">-->
          <div class="caption">
            <h4 style="color:#7031BE"><%=s.name%></h4>
            <h6><%= s.location %></h6>
            <h6><%=s.date.strftime("%b %e, %Y at %I:%M%p") %></h6>
            <h6><%=s.description%></h6>
            <!--<p><a href="#" class="btn btn-primary" role="button">Button</a> -->
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>